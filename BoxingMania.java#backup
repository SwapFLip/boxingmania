                    opponentMove = 5;
                }
            }
        if (opponentMove != 8 || opponentMove != 9 || opponentMove != 10)
            if (opponentHealth <= 135) {
                opponentMove = (int)(Math.random() * 16) + 1;
                while (opponentMove == 1 && opponentenergy < 20 || opponentMove == 2 && opponentenergy < 10 || opponentMove == 3 && opponentenergy < 80 || opponentMove == 4 && opponentenergy < 5 || opponentMove == 6 && opponentenergy < 15 || opponentMove == 7 && opponentenergy < 20 || opponentMove == 8 && opponentenergy < 20 || opponentMove == 11 && opponentenergy < 20 || opponentMove == 13 && opponentenergy < 20 || opponentMove == 14 && opponentenergy < 20 || opponentMove == 15 && opponentenergy < 20 || opponentMove == 16 && opponentenergy < 20) {
