        if (rage_meter >= 0) {
            opponentHealth -= playerKick;
            BoxingMania.playSound("data/normalpunch.wav");
            BoxingMania.playSound("data/gethit.wav");
        } else {
            playerenergy -= 20;
            opponentHealth -= playerKick;
            if (crit >= 700) {
                opponentHealth -= 10;
                BoxingMania.playSound("data/normalpunch.wav");
                BoxingMania.playSound("data/gethit.wav");
            } else {
                BoxingMania.playSound("data/normalpunch.wav");
                BoxingMania.playSound("data/gethit.wav");
            }
        }
