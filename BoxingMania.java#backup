    private static void start5() throws InterruptedException {
        System.exit(0);
    }
    static int q1 = 0, q2 = 0, q3 = 0, q4 = 0;
    private static void start6() throws InterruptedException {
        Scanner in = new Scanner(System.in);
        try {
            File file = new File("c1.txt");
            Scanner sc = new Scanner(file);
            q1 = sc.nextInt();
        } catch (FileNotFoundException e) {}
        try {
            File file = new File("c2.txt");
            Scanner sc = new Scanner(file);
            q2 = sc.nextInt();
        } catch (FileNotFoundException e) {}
        try {
            File file = new File("c3.txt");
            Scanner sc = new Scanner(file);
            q3 = sc.nextInt();
        } catch (FileNotFoundException e) {}
        try {
            File file = new File("c4.txt");
            Scanner sc = new Scanner(file);
            q4 = sc.nextInt();
        } catch (FileNotFoundException e) {}
        try {
            File file = new File("s1.txt");
            Scanner sc = new Scanner(file);
            c1 = sc.nextInt();
        } catch (FileNotFoundException e) {}
        try {
            File file = new File("s2.txt");
            Scanner sc = new Scanner(file);
            c2 = sc.nextInt();
        } catch (FileNotFoundException e) {}
        try {
            File file = new File("s3.txt");
            Scanner sc = new Scanner(file);
            c3 = sc.nextInt();
        } catch (FileNotFoundException e) {}
        try {
            File file = new File("s4.txt");
            Scanner sc = new Scanner(file);
            c4 = sc.nextInt();
        } catch (FileNotFoundException e) {}
        try {
            File file = new File("s5.txt");
            Scanner sc = new Scanner(file);
            c5 = sc.nextInt();
        } catch (FileNotFoundException e) {}
        try {
            File file = new File("s6.txt");
            Scanner sc = new Scanner(file);
            c6 = sc.nextInt();
        } catch (FileNotFoundException e) {}
        try {
            File file = new File("s7.txt");
            Scanner sc = new Scanner(file);
            c7 = sc.nextInt();
        } catch (FileNotFoundException e) {}
        try {
            File file = new File("s8.txt");
            Scanner sc = new Scanner(file);
            c8 = sc.nextInt();
        } catch (FileNotFoundException e) {}
        int ssave = 99;
        while (ssave != 1 && ssave != 2 && ssave != 3 && ssave != 4 && ssave != 10) {
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
            System.out.println("|____________________________________SAVE GAME___________________10) Back______|");
            System.out.println("|                                                       BCoins:" + q1 + "                |");
            System.out.println("|  SAVE GAME                                            Wins:" + c1 + "                |");
            System.out.println("|      1                                                Losses:" + c2 + "              |");
            System.out.println("|______________________________________________________________________________|");
            System.out.println("|                                                       BCoins:" + q2 + "                |");
            System.out.println("|  SAVE GAME                                            Wins:" + c3 + "                |");
            System.out.println("|      2                                                Losses:" + c4 + "              |");
            System.out.println("|______________________________________________________________________________|");
            System.out.println("|                                                       BCoins:" + q3 + "                |");
            System.out.println("|  SAVE GAME                                            Wins:" + c5 + "                |");
            System.out.println("|      3                                                Losses:" + c6 + "              |");
            System.out.println("|______________________________________________________________________________|");
            System.out.println("|                                                       BCoins:" + q4 + "                |");
            System.out.println("|  SAVE GAME                                            Wins:" + c7 + "                |");
            System.out.println("|      4                                                Losses:" + c8 + "              |");
            System.out.println("|______________________________________________________________________________|");
            System.out.println("--------------------------------------------------------------------------------");
            ssave = in.nextInt();
        }
        if (ssave == 1) {
            BoxingMania.save1();
        } else if (ssave == 2) {
            BoxingMania.save2();
        } else if (ssave == 3) {
            BoxingMania.save3();
        } else if (ssave == 4) {
            BoxingMania.save4();
        } else if (ssave == 10) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/menu.wav");
            BoxingMania.mainmenu();
        }
    }
    private static void save1() throws InterruptedException {
        Scanner in = new Scanner(System.in);
        int s1 = 99;
        while (s1 != 1 && s1 != 2) {
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
            System.out.println("|                                                                              |");
            System.out.println("|                                  SAVE GAME 1                                 |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|     Wins: " + c1 + "                                                                 |");
            System.out.println("|     Losses: " + c2 + "                                                               |");
