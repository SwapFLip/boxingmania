                        playerHealth = 100;
                        playerJab *= 2;
                        playerHook *= 2;
                        playerUppercut *= 2;
                        playerFeint *= 2;
                        playerSweep *= 2;
                        playerKick *= 2;
                        playerHeal *= 2;
                        rage_meter = 3;
                    }
                    rage_count++;
                }
                rage_meter--;
                if (rage_meter == 0) {
                    playerJab /= 2;
                    playerHook /= 2;
                    playerUppercut /= 2;
                    playerFeint /= 2;
                    playerSweep /= 2;
                    playerKick /= 2;
                    playerHeal /= 2;
                    playerHealth -= 10;
                }
            }
            moves++;
            playerHealth++;
            opponentHealth++;
            if (playerHealth >= 1000 && playerNumber != 22) playerHealth = 1000;
            if (opponentHealth >= 1000 && opponentNumber != 22 && (!opponentname.equals("PROTOTYPE"))) opponentHealth = 1000;
            secretBOSSFIGHT = 0;
        }
    }

    private static void ailvnoob() throws InterruptedException {
        opponentMove = (int)(Math.random() * 8) + 1;
        while (opponentMove == 1 && opponentenergy < 20 || opponentMove == 2 && opponentenergy < 10 || opponentMove == 3 && opponentenergy < 80 || opponentMove == 4 && opponentenergy < 5 || opponentMove == 6 && opponentenergy < 15 || opponentMove == 7 && opponentenergy < 20 || opponentMove == 8 && opponentenergy < 20) {
            opponentMove = (int)(Math.random() * 8) + 1;
        }
    }

    private static void ailv1() throws InterruptedException {
        opponentMove = (int)(Math.random() * 8) + 1;
        while (opponentMove == 1 && opponentenergy < 20 || opponentMove == 2 && opponentenergy < 10 || opponentMove == 3 && opponentenergy < 80 || opponentMove == 4 && opponentenergy < 5 || opponentMove == 6 && opponentenergy < 15 || opponentMove == 7 && opponentenergy < 20 || opponentMove == 8 && opponentenergy < 20) {
            opponentMove = (int)(Math.random() * 8) + 1;
        }
        if (playerHealth <= 20) {
            opponentMove = (int)(Math.random() * 20) + 1;
            while (opponentMove == 1 && opponentenergy < 20 || opponentMove == 2 && opponentenergy < 10 || opponentMove == 3 && opponentenergy < 80 || opponentMove == 4 && opponentenergy < 5 || opponentMove == 6 && opponentenergy < 15 || opponentMove == 7 && opponentenergy < 20 || opponentMove == 8 && opponentenergy < 20 || opponentMove == 11 && opponentenergy < 20 || opponentMove == 13 && opponentenergy < 80 || opponentMove == 14 && opponentenergy < 80 || opponentMove == 15 && opponentenergy < 80 || opponentMove == 16 && opponentenergy < 20 || opponentMove == 17 && opponentenergy < 80 || opponentMove == 18 && opponentenergy < 80 || opponentMove == 19 && opponentenergy < 80 || opponentMove == 20 && opponentenergy < 20) {
                opponentMove = (int)(Math.random() * 20) + 1;
            }
            if (opponentMove == 13 || opponentMove == 14) {
                opponentMove = 3;
            } else if (opponentMove == 16 || opponentMove == 20 || opponentMove == 18 || opponentMove == 17 || opponentMove == 15 || opponentMove == 19) {
                opponentMove = 1;
            }
        }
        if (opponentenergy <= 20) {
            opponentMove = (int)(Math.random() * 12) + 1;
            while (opponentMove == 1 && opponentenergy < 20 || opponentMove == 2 && opponentenergy < 10 || opponentMove == 3 && opponentenergy < 80 || opponentMove == 4 && opponentenergy < 5 || opponentMove == 6 && opponentenergy < 15 || opponentMove == 7 && opponentenergy < 20 || opponentMove == 8 && opponentenergy < 20 || opponentMove == 11 && opponentenergy < 20) {
                opponentMove = (int)(Math.random() * 12) + 1;
            }
            if (opponentMove == 5 || opponentMove == 9 || opponentMove == 10 || opponentMove == 12) {
                opponentMove = 5;
            }
        }
        if (opponentHealth <= 30) {
            opponentMove = (int)(Math.random() * 16) + 1;
            while (opponentMove == 1 && opponentenergy < 20 || opponentMove == 2 && opponentenergy < 10 || opponentMove == 3 && opponentenergy < 80 || opponentMove == 4 && opponentenergy < 5 || opponentMove == 6 && opponentenergy < 15 || opponentMove == 7 && opponentenergy < 20 || opponentMove == 8 && opponentenergy < 20 || opponentMove == 11 && opponentenergy < 20 || opponentMove == 13 && opponentenergy < 20 || opponentMove == 14 && opponentenergy < 20 || opponentMove == 15 && opponentenergy < 20 || opponentMove == 16 && opponentenergy < 20) {
                opponentMove = (int)(Math.random() * 16) + 1;
            }
            if (opponentMove == 8 || opponentMove == 11 || opponentMove == 13 || opponentMove == 14 || opponentMove == 15 || opponentMove == 16) {
                opponentMove = 8;
            }
        }
    }

    private static void ailv2() throws InterruptedException {
        opponentMove = (int)(Math.random() * 8) + 1;
        while (opponentMove == 1 && opponentenergy < 20 || opponentMove == 2 && opponentenergy < 10 || opponentMove == 3 && opponentenergy < 80 || opponentMove == 4 && opponentenergy < 5 || opponentMove == 6 && opponentenergy < 15 || opponentMove == 7 && opponentenergy < 20 || opponentMove == 8 && opponentenergy < 20) {
            opponentMove = (int)(Math.random() * 8) + 1;
        }
        if (playerHealth <= 50) {
            opponentMove = (int)(Math.random() * 20) + 1;
            while (opponentMove == 1 && opponentenergy < 20 || opponentMove == 2 && opponentenergy < 10 || opponentMove == 3 && opponentenergy < 80 || opponentMove == 4 && opponentenergy < 5 || opponentMove == 6 && opponentenergy < 15 || opponentMove == 7 && opponentenergy < 20 || opponentMove == 8 && opponentenergy < 20 || opponentMove == 11 && opponentenergy < 20 || opponentMove == 13 && opponentenergy < 80 || opponentMove == 14 && opponentenergy < 80 || opponentMove == 15 && opponentenergy < 80 || opponentMove == 16 && opponentenergy < 20 || opponentMove == 17 && opponentenergy < 80 || opponentMove == 18 && opponentenergy < 80 || opponentMove == 19 && opponentenergy < 80 || opponentMove == 20 && opponentenergy < 20) {
                opponentMove = (int)(Math.random() * 20) + 1;
            }
            if (opponentMove == 13 || opponentMove == 14 || opponentMove == 15 || opponentMove == 19) {
                opponentMove = 3;
            } else if (opponentMove == 16 || opponentMove == 20 || opponentMove == 18 || opponentMove == 17) {
                opponentMove = 1;
            }
        }
        if (opponentenergy <= 35) {
            opponentMove = (int)(Math.random() * 12) + 1;
            while (opponentMove == 1 && opponentenergy < 20 || opponentMove == 2 && opponentenergy < 10 || opponentMove == 3 && opponentenergy < 80 || opponentMove == 4 && opponentenergy < 5 || opponentMove == 6 && opponentenergy < 15 || opponentMove == 7 && opponentenergy < 20 || opponentMove == 8 && opponentenergy < 20 || opponentMove == 11 && opponentenergy < 20) {
                opponentMove = (int)(Math.random() * 12) + 1;
            }
            if (opponentMove == 5 || opponentMove == 9 || opponentMove == 10 || opponentMove == 12) {
                opponentMove = 5;
            }
        }
        if (opponentHealth <= 50) {
            opponentMove = (int)(Math.random() * 16) + 1;
            while (opponentMove == 1 && opponentenergy < 20 || opponentMove == 2 && opponentenergy < 10 || opponentMove == 3 && opponentenergy < 80 || opponentMove == 4 && opponentenergy < 5 || opponentMove == 6 && opponentenergy < 15 || opponentMove == 7 && opponentenergy < 20 || opponentMove == 8 && opponentenergy < 20 || opponentMove == 11 && opponentenergy < 20 || opponentMove == 13 && opponentenergy < 20 || opponentMove == 14 && opponentenergy < 20 || opponentMove == 15 && opponentenergy < 20 || opponentMove == 16 && opponentenergy < 20) {
                opponentMove = (int)(Math.random() * 16) + 1;
            }
            if (opponentMove == 8 || opponentMove == 11 || opponentMove == 13 || opponentMove == 14 || opponentMove == 15 || opponentMove == 16) {
                opponentMove = 8;
            }
        }
    }
    private static void ailv3() throws InterruptedException {
        opponentMove = (int)(Math.random() * 8) + 1;
        while (opponentMove == 1 && opponentenergy < 20 || opponentMove == 2 && opponentenergy < 10 || opponentMove == 3 && opponentenergy < 80 || opponentMove == 4 && opponentenergy < 5 || opponentMove == 6 && opponentenergy < 15 || opponentMove == 7 && opponentenergy < 20 || opponentMove == 8 && opponentenergy < 20) {
            opponentMove = (int)(Math.random() * 8) + 1;
        }
        if (playerHealth <= 80) {
            opponentMove = (int)(Math.random() * 20) + 1;
            while (opponentMove == 1 && opponentenergy < 20 || opponentMove == 2 && opponentenergy < 10 || opponentMove == 3 && opponentenergy < 80 || opponentMove == 4 && opponentenergy < 5 || opponentMove == 6 && opponentenergy < 15 || opponentMove == 7 && opponentenergy < 20 || opponentMove == 8 && opponentenergy < 20 || opponentMove == 11 && opponentenergy < 20 || opponentMove == 13 && opponentenergy < 80 || opponentMove == 14 && opponentenergy < 80 || opponentMove == 15 && opponentenergy < 80 || opponentMove == 16 && opponentenergy < 20 || opponentMove == 17 && opponentenergy < 80 || opponentMove == 18 && opponentenergy < 80 || opponentMove == 19 && opponentenergy < 80 || opponentMove == 20 && opponentenergy < 20) {
                opponentMove = (int)(Math.random() * 20) + 1;
            }
            if (opponentMove == 13 || opponentMove == 14 || opponentMove == 15 || opponentMove == 18 || opponentMove == 17 || opponentMove == 19) {
                opponentMove = 3;
            } else if (opponentMove == 16 || opponentMove == 20) {
                opponentMove = 1;
            }
        }
