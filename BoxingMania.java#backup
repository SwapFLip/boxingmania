         System.out.println("|                   " + playerHealth + " HP                                   " + opponentHealth + " HP            |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|            |==============|                         <==============>         |");
        System.out.println("|            |              |                         |              |         |");
        System.out.println("|            | " + name + " |                         |  " + ooface + "  |         |");
        System.out.println("|           (                )                    _  [                ]        |");
        System.out.println("|            |      " + nose + "      |                    [_]__|      ==      |         |");
        System.out.println("|            |    " + face + "    |                    |____|   " + oomouth + "   |         |");
        System.out.println("|            |______________|                         !______________!         |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
