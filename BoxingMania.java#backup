            System.out.println("|            |______________|                         !______________!         |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                          Opponent did a critical attack                      |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("--------------------------------------------------------------------------------");
            try {
                Thread.sleep(2000);
            } catch (InterruptedException e) {
                e.printStackTrace();
            }
        }
    }
    private static void omove8() throws InterruptedException {
        opponentenergy -= 20;
        System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
        System.out.println("--------------------------------------------------------------------------------");
