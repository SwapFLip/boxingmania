    static int tourna = 0;
    private static void tournament() throws InterruptedException {
        Scanner in = new Scanner(System.in);
        tourna++;
        long diffc = 0;
        oface = (int)(Math.random() * 10) + 1;
        omouth = (int)(Math.random() * 10) + 1;
        if (oface == 1) ooface = oeyes1;
        else if (oface == 2) ooface = oeyes2;
        else if (oface == 3) ooface = oeyes3;
        else if (oface == 4) ooface = oeyes4;
        else if (oface == 5) ooface = oeyes5;
        else if (oface == 6) ooface = oeyes6;
        else if (oface == 7) ooface = oeyes7;
        else if (oface == 8) ooface = oeyes8;
        else if (oface == 9) ooface = oeyes9;
        else if (oface == 10) ooface = oeyes10;
        if (omouth == 1) oomouth = omouth1;
        else if (omouth == 2) oomouth = omouth2;
        else if (omouth == 3) oomouth = omouth3;
        else if (omouth == 4) oomouth = omouth4;
        else if (omouth == 5) oomouth = omouth5;
        else if (omouth == 6) oomouth = omouth6;
        else if (omouth == 7) oomouth = omouth7;
        else if (omouth == 8) oomouth = omouth8;
        else if (omouth == 9) oomouth = omouth9;
        else if (omouth == 10) oomouth = omouth10;
        if (tourna == 1) {
            String inpu = "-1";
            diffc = coins;
            playerenergy = 200;
            while ((!inpu.equals("1")) && (!inpu.equals("2")) && (!inpu.equals("3")) && (!inpu.equals("4")) && (!inpu.equals("10"))) {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("|                                  TOURNAMENT                                  |");
                System.out.println("|------------------------------------------------------------------------------|");
                System.out.println("|                                                                              |");
                System.out.println("|    Select your Difficulty :                                                  |");
                System.out.println("|                                                                              |");
                System.out.println("|    1) NOOB                                                                   |");
                System.out.println("|                                           Higher Difficulty Gives            |");
                System.out.println("|    2) EASY                                Higher BCoins!                     |");
                System.out.println("|                                                                              |");
                System.out.println("|    3) MEDIUM                                                                 |");
                System.out.println("|                                           You have to fight 3 fighters       |");
                System.out.println("|    4) HARD                               continuously to win                |");
                System.out.println("|                                                                              |");
                System.out.println("|    5) CRAZY                                                                  |");
                System.out.println("|                                                                              |");
                System.out.println("|                                   GOOD LUCK                                  |");
                System.out.println("|                                                                     10) Menu |");
                System.out.println("--------------------------------------------------------------------------------");
                inpu = in.nextLine().trim();
            }
            if (inpu.equals("1"))
                botaistrength = 0;
            else if (inpu.equals("2"))
                botaistrength = 1;
            else if (inpu.equals("3"))
                botaistrength = 2;
            else if (inpu.equals("4"))
                botaistrength = 3;
            else if (inpu.equals("5"))
                botaistrength = 4;
            else if (inpu.equals("10"))
                mainmenu();
            start1();
        }
        if (tourna == 2) {
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                      ROUND 1                                 |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("--------------------------------------------------------------------------------");
            try {
                Thread.sleep(4000);
            } catch (InterruptedException e) {
                e.printStackTrace();
            }
            opponentHealth = op1[0];
            opponentJab = op1[1];
            opponentHook = op1[2];
            opponentUppercut = op1[3];
            opponentFeint = op1[4];
            opponentSweep = op1[5];
            opponentKick = op1[6];
            opponentHeal = op1[7];
            opponentname = op1n;
            tournafight();
        } else if (tourna == 3) {
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
