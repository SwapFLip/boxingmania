        if (rage_meter >= 0) {
            opponentHealth -= playerSweep;
            BoxingMania.playSound("data/normalpunch.wav");
            BoxingMania.playSound("data/gethit.wav");
        } else {
            playerenergy -= 15;
            opponentHealth -= playerSweep;
            if (crit >= 700) {
                opponentHealth -= 10;
                BoxingMania.playSound("data/normalpunch.wav");
                BoxingMania.playSound("data/gethit.wav");
            } else {
                BoxingMania.playSound("data/normalpunch.wav");
                BoxingMania.playSound("data/gethit.wav");
            }
