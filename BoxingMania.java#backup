                Scanner sc = new Scanner(file);
                name = sc.nextLine();
            } catch (FileNotFoundException e) {}
            try {
                File file = new File("pn4.txt");
                Scanner sc = new Scanner(file);
                nose = sc.nextLine();
            } catch (FileNotFoundException e) {}
            try {
                File file = new File("pm4.txt");
                Scanner sc = new Scanner(file);
                face = sc.nextLine();
            } catch (FileNotFoundException e) {}
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                          GAME LOADED FROM SAVE GAME 4                        |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("--------------------------------------------------------------------------------");
            try {
                Thread.sleep(2000);
            } catch (InterruptedException e) {
                e.printStackTrace();
            }
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/menu.wav");
            BoxingMania.mainmenu();
        }
        BoxingMania.stopSound();
        BoxingMania.playloopSound("data/menu.wav");
        BoxingMania.mainmenu();
    }
    private static void pause() throws InterruptedException {
        Scanner in = new Scanner(System.in);
        String a = "99";
        while (!a.equals("1") && !a.equals("2") && !a.equals("3") && !a.equals("4")) {
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
            System.out.println("|                                                                              |");
            System.out.println("|                                  GAME PAUSED                                 |");
            System.out.println("|------------------------------------------------------------------------------|");
            System.out.println("|                                                                              |");
            System.out.println("|                               1) RESUME GAME                                 |");
            System.out.println("|                                                                              |");
            System.out.println("|                                2) Main Menu                                  |");
            System.out.println("|                                                                              |");
            System.out.println("|                                3) Exit Game                                  |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                               4) Simple Tips |");
            System.out.println("--------------------------------------------------------------------------------");
            a = in.nextLine();
        }
        if (a.equals("1")) {} else if (a.equals("2")) {
            BoxingMania.playloopSound("data/menu.wav");
            BoxingMania.mainmenu();
        } else if (a.equals("3")) {
            System.exit(0);
        } else if (a.equals("4")) {
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
            System.out.println("|                                                                              |");
            System.out.println("|                                  Simple Tips                                 |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|   Blocking gives you energy!                                                 |");
            System.out.println("|                                                                              |");
            System.out.println("|               Blocking takes health from you!                                |");
            System.out.println("|                                                                              |");
            System.out.println("|                         You have options to choose harder difficulty         |");
            System.out.println("|                                                                              |");
            System.out.println("|                                      You can buy boosters and use them       |");
            System.out.println("|                                                                              |");
            System.out.println("|   More Energy Use = More Damage                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                Winning Gives you BCoins                                      |");
            System.out.println("|                                                                              |");
            System.out.println("|                          Press Enter to Go Back to game!                     |");
            System.out.println("--------------------------------------------------------------------------------");
            in.nextLine();
        }
        BoxingMania.stopSound();
        fights = (int)(Math.random() * 3) + 1;
        if (fights == 1) BoxingMania.playloopSound("data/fight.wav");
        else if (fights == 2) BoxingMania.playloopSound("data/fight2.wav");
        else if (fights == 3) BoxingMania.playloopSound("data/fight3.wav");
    }
    private static void start7() throws InterruptedException {
        //aiwatch
        playerenergy = 200;
        opponentenergy = 200;
        winner = 0;
        moves = 0;
        BoxingMania.begins();
        if (battlemusic == 1) BoxingMania.playloopSound("data/fight.wav");
        else if (battlemusic == 2) BoxingMania.playloopSound("data/fight2.wav");
        else if (battlemusic == 3) BoxingMania.playloopSound("data/fight3.wav");
        else if (battlemusic == 4) {
            fights = (int)(Math.random() * 3) + 1;
            if (fights == 1) BoxingMania.playloopSound("data/fight.wav");
            else if (fights == 2) BoxingMania.playloopSound("data/fight2.wav");
            else if (fights == 3) BoxingMania.playloopSound("data/fight3.wav");
        }
        battlemusic = 1;
        Scanner in = new Scanner(System.in);
        playerMove = 0;
        System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
        System.out.println("--------------------------------------------------------------------------------");
        System.out.println("|                                  Bot VS Bot                                  |");
        System.out.println("|------------------------------------------------------------------------------|");
        System.out.println("|                                                                              |");
        System.out.println("|    Select your Bot 1 AI Level :                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|    1) NOOB                                                                   |");
        System.out.println("|                                                                              |");
        System.out.println("|    2) EASY                                                                   |");
        System.out.println("|                                                                              |");
        System.out.println("|    3) MEDIUM                               ENTER BOT 1 AI LEVEL              |");
        System.out.println("|                                                                              |");
        System.out.println("|    4) HARD                                                                   |");
        System.out.println("|                                                                              |");
        System.out.println("|    5) CRAZY                                                                  |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                Enter your choice:                   10) Menu |");
        System.out.println("--------------------------------------------------------------------------------");
        int a = in.nextInt();
        while (a != 1 && a != 2 && a != 3 && a != 4 && a != 5)
            a = in.nextInt();
        System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
        System.out.println("--------------------------------------------------------------------------------");
        System.out.println("|                                  Bot VS Bot                                  |");
        System.out.println("|------------------------------------------------------------------------------|");
        System.out.println("|                                                                              |");
        System.out.println("|    Select your Bot 2 AI Level :                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|    1) NOOB                                                                   |");
        System.out.println("|                                                                              |");
        System.out.println("|    2) EASY                                                                   |");
        System.out.println("|                                                                              |");
        System.out.println("|    3) MEDIUM                               ENTER BOT 2 AI LEVEL              |");
        System.out.println("|                                                                              |");
        System.out.println("|    4) HARD                                                                   |");
        System.out.println("|                                                                              |");
        System.out.println("|    5) CRAZY                                                                  |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                Enter your choice:                   10) Menu |");
        System.out.println("--------------------------------------------------------------------------------");
        int b = in.nextInt();
        while (b != 1 && b != 2 && b != 3 && b != 4 && b != 5)
            b = in.nextInt();
        while (true) {
            if (a == 1)
                BoxingMania.playerlvnoob();
            else if (a == 2)
                BoxingMania.playerlv1();
            else if (a == 3)
                BoxingMania.playerlv2();
            else if (a == 4)
                BoxingMania.playerlv3();
            else if (a == 5)
                BoxingMania.playerlv4();
            if (b == 1)
                BoxingMania.ailvnoob();
            else if (b == 2)
                BoxingMania.ailv1();
            else if (b == 3)
                BoxingMania.ailv2();
            else if (b == 4)
                BoxingMania.ailv3();
            else if (b == 5)
                BoxingMania.ailv4();
            if (opponentMove != 5) {
                crit = (int)(Math.random() * 1000) + 1;
                if (playerMove == 1) {
                    BoxingMania.move1();
                } else if (playerMove == 2) {
                    BoxingMania.move2();
                } else if (playerMove == 3) {
                    BoxingMania.move3();
                } else if (playerMove == 4) {
                    BoxingMania.move4();
                } else if (playerMove == 6) {
                    BoxingMania.move6();
                } else if (playerMove == 7) {
                    BoxingMania.move7();
                } else if (playerMove == 8) {
                    BoxingMania.move8();
                }
            } else if (playerMove == 8) {
                BoxingMania.move8();
            }
            if (playerHealth <= 0) {
