    private static void start2() throws InterruptedException {
        Scanner in = new Scanner(System.in);
        System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
        System.out.println("--------------------------------------------------------------------------------");
        System.out.println("|                                                                              |");
        System.out.println("|   The Controls are all from the Keyboard no Mouse to be Used!                |");
        System.out.println("|   The Basic conntrols are all used in the keys 1-8                           |");
        System.out.println("|   Just Put the Number and Press Enter to GO!                                 |");
        System.out.println("|   The opponent and the player both gain health slowly                        |");
        System.out.println("|   and also regain some energy over time, you can also regain energy by       |");
        System.out.println("|   blocking. For Bosses there is a rage system which acivates at 100 or less  |");
        System.out.println("|   health. The bosses are tough so make sure to do some critical hits!        |");
        System.out.println("|                             [O]     [O]                                      |");
        System.out.println("|                                                                              |");
        System.out.println("|                                  O                                           |");
        System.out.println("|                                                                              |");
        System.out.println("|                                |____|                                        |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                        Press Enter to go to main menu                        |");
        System.out.println("--------------------------------------------------------------------------------");
        in.nextLine();
        BoxingMania.stopSound();
        BoxingMania.playloopSound("data/menu.wav");
        BoxingMania.mainmenu();
    }

