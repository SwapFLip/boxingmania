        if (rage_meter >= 0) {
            playerenergy -= 20;
            opponentHealth -= playerUppercut;
            BoxingMania.playSound("data/normalpunch.wav");
            BoxingMania.playSound("data/gethit.wav");
        } else {
            playerenergy -= 80;
            opponentHealth -= playerUppercut;
            playerHealth-=20;
            if (crit >= 700) {
                opponentHealth -= 10;
                BoxingMania.playSound("data/normalpunch.wav");
                BoxingMania.playSound("data/gethit.wav");
            } else {
                BoxingMania.playSound("data/normalpunch.wav");
                BoxingMania.playSound("data/gethit.wav");
            }
        }
        try {
            Thread.sleep(170);
        } catch (InterruptedException e) {
            e.printStackTrace();
        }
