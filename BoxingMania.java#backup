    private static void mainmenu() throws InterruptedException {
        Scanner in = new Scanner(System.in);
        ooface = oeyes2;
        oomouth = omouth4;
        Date currentDate = new Date();
        tourna = 0;
        SimpleDateFormat dateFormat = new SimpleDateFormat("MM-dd");
        String formattedDate = dateFormat.format(currentDate);
        SimpleDateFormat datef = new SimpleDateFormat("MM");
        String formatdate = datef.format(currentDate);
        String startthegame = "p";
        while ((!startthegame.equals("1")) && (!startthegame.equals("2")) && (!startthegame.equals("3")) && (!startthegame.equals("4")) && (!startthegame.equals("5")) && (!startthegame.equals("6")) && (!startthegame.equals("10"))) {
            if (formattedDate.equals("10-04")) {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("| 10) Achievements                                                  6) Others  |");
                System.out.println("|         ______             _                ______              _            |");
                System.out.println("|        (  __  |           (_)              |  ___ |            (_)           |");
                System.out.println("|        | |__)  ) ___ _   _ _ ____   ____   | | _ | | ____ ____  _  ____      |");
                System.out.println("|        |  __  ( / _ ( | / ) |  _ | / _  |  | || || |/ _  |  _ || |/ _  |     |");
                System.out.println("|        | |__)  ) |_| ) X (| | | | ( ( | |  | || || ( ( | | | | | ( ( | |     |");
                System.out.println("|        |______/ |___(_/ |_)_|_| |_||_|| |  |_||_||_||_||_|_| |_|_||_||_|V1.9 |");
                System.out.println("|                                   (_____|                                    |");
                System.out.println("|                                                         Wins = " + BoxingMania.savevalue1 + "             |");
                System.out.println("|   HAPPY BIRTHDAY SWAPFLIP                               Losses = " + BoxingMania.savevalue2 + "           |");
                System.out.println("|       AKA DEVELOPER                                                          |");
                System.out.println("|    AKA SWAPNANEEL DUTTA                                                      |");
                System.out.println("|                               1) Start the Game        BCoins = " + coins + "            |");
                System.out.println("|  WISH HIM A HAPPY BIRTHDAY                                                   |");
                System.out.println("|                                 2) Tutorial                                  |");
                System.out.println("|                                                                              |");
                System.out.println("| 5) Exit                         3) Credits                  4) Save Game     |");
                System.out.println("--------------------------------------------------------------------------------");
            } else if (formatdate.equals("03")) {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("| 10) Achievements         HAPPY BIRTHMONTH BOXING MANIA            6) Others  |");
                System.out.println("|         ______             _                ______              _            |");
                System.out.println("|        (  __  |           (_)              |  ___ |            (_)           |");
                System.out.println("|        | |__)  ) ___ _   _ _ ____   ____   | | _ | | ____ ____  _  ____      |");
                System.out.println("|        |  __  ( / _ ( | / ) |  _ | / _  |  | || || |/ _  |  _ || |/ _  |     |");
                System.out.println("|        | |__)  ) |_| ) X (| | | | ( ( | |  | || || ( ( | | | | | ( ( | |     |");
                System.out.println("|        |______/ |___(_/ |_)_|_| |_||_|| |  |_||_||_||_||_|_| |_|_||_||_|V1.9 |");
                System.out.println("|HAPPY BIRTHMONTH BOXING MANIA      (_____|      HAPPY BIRTHMONTH BOXING MANIA |");
                System.out.println("|    |==============|      HAPPY BIRTHMONTH BOXING MANIA  Wins = " + BoxingMania.savevalue1 + "             |");
                System.out.println("|    |              |      HAPPY BIRTHMONTH BOXING MANIA  Losses = " + BoxingMania.savevalue2 + "           |");
                System.out.println("|    | " + name + " |      HAPPY BIRTHMONTH BOXING MANIA                       |");
                System.out.println("|   (                )                                                         |");
                System.out.println("|    |      " + nose + "      |           1) Start the Game        BCoins = " + coins + "            |");
                System.out.println("|    |    " + face + "    |                                                          |");
                System.out.println("|    |______________|             2) Tutorial                                  |");
                System.out.println("|                                                                              |");
                System.out.println("| 5) Exit                         3) Credits                  4) Save Game     |");
                System.out.println("--------------------------------------------------------------------------------");
            } else {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("| 10) Achievements                                                  6) Others  |");
                System.out.println("|         ______             _                ______              _            |");
                System.out.println("|        (  __  |           (_)              |  ___ |            (_)           |");
                System.out.println("|        | |__)  ) ___ _   _ _ ____   ____   | | _ | | ____ ____  _  ____      |");
                System.out.println("|        |  __  ( / _ ( | / ) |  _ | / _  |  | || || |/ _  |  _ || |/ _  |     |");
                System.out.println("|        | |__)  ) |_| ) X (| | | | ( ( | |  | || || ( ( | | | | | ( ( | |     |");
                System.out.println("|        |______/ |___(_/ |_)_|_| |_||_|| |  |_||_||_||_||_|_| |_|_||_||_|V1.9 |");
                System.out.println("|                                   (_____|                                    |");
                System.out.println("|    |==============|                                     Wins = " + BoxingMania.savevalue1 + "             |");
                System.out.println("|    |              |                                     Losses = " + BoxingMania.savevalue2 + "           |");
                System.out.println("|    | " + name + " |                                                          |");
                System.out.println("|   (                )                                                         |");
                System.out.println("|    |      " + nose + "      |            1) Start Game           BCoins = " + coins + "            |");
                System.out.println("|    |    " + face + "    |                                                          |");
                System.out.println("|    |______________|             2) Tutorial                                  |");
                System.out.println("|                                                                              |");
                System.out.println("| 5) Exit                         3) Credits                  4) Save Game     |");
                System.out.println("--------------------------------------------------------------------------------");
            }
            startthegame = in.nextLine();
            if (startthegame.equals("DEATH") && secretBOSSFIGHT == 3) {
                secretBOSSFIGHT = 4;
                startthegame = "1";
            } else {
                secretBOSSFIGHT = 0;
            }
        }
        if (startthegame.equals("1")) {
            String go = "0";
            while ((!go.equals("1")) && (!go.equals("2")) && (!go.equals("3")) && (!go.equals("4")) && (!go.equals("10")) && (!go.equals("5")) && (!go.equals("9"))) {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("| 9) Last Match Result                                                         |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                               1) Player Vs Bot               (Only Player    |");
                System.out.println("|                                                               vs Bot gives   |");
                System.out.println("|                                                               score)         |");
                System.out.println("|                               2) Player Vs Player                            |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                               3) Bot Vs Bot                                  |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                               4) Training                                    |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                               5) Tournament                                  |");
                System.out.println("|                                                                    10) Menu  |");
                System.out.println("--------------------------------------------------------------------------------");
                go = in.nextLine();
            }
            if (go.equals("1")) start = 1;
            else if (go.equals("2")) start = 9;
            else if (go.equals("3")) start = 7;
            else if (go.equals("4")) start = 8;
            else if (go.equals("10")) BoxingMania.mainmenu();
            else if (go.equals("5")) BoxingMania.tournament();
            else if (go.equals("9")) BoxingMania.replay();
            if (start == 1) {
                BoxingMania.playSound("data/click.wav");
                BoxingMania.start1();
            } else if (start == 7) {
                BoxingMania.playSound("data/click.wav");
                secretBOSSFIGHT = 0;
                BoxingMania.start1();
            } else if (start == 8) {
                secretBOSSFIGHT = 0;
                BoxingMania.playSound("data/click.wav");
                BoxingMania.start8();
            } else if (start == 9) {
                secretBOSSFIGHT = 0;
                BoxingMania.playSound("data/click.wav");
                BoxingMania.start1();
            }
        } else if (startthegame.equals("2")) {
            secretBOSSFIGHT = 0;
            String tut = "0";
            while ((!tut.equals("1")) && (!tut.equals("2"))) {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                               1) Manual Tutorial (Outdated)                  |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                              2) Automatic Tutorial                           |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("--------------------------------------------------------------------------------");
                tut = in.nextLine();
            }
            if (tut.equals("1")) {
                BoxingMania.playSound("data/click.wav");
                BoxingMania.start2();
            } else if (tut.equals("2")) {
                BoxingMania.playSound("data/click.wav");
                BoxingMania.start4();
            }
        } else if (startthegame.equals("3")) {
            BoxingMania.playSound("data/click.wav");
            BoxingMania.start3();
        } else if (startthegame.equals("4")) {
            BoxingMania.playSound("data/click.wav");
            BoxingMania.start6();
        } else if (startthegame.equals("5")) {
            BoxingMania.playSound("data/click.wav");
            BoxingMania.start5();
        } else if (startthegame.equals("6")) {
            BoxingMania.playSound("data/click.wav");
            BoxingMania.others();
        } else if (startthegame.equals("10")) {
            BoxingMania.playSound("data/click.wav");
            BoxingMania.start10();
        } else if ((startthegame.equals("c")) || (startthegame.equals("C"))) {
            BoxingMania.playSound("data/click.wav");
            BoxingMania.start10();
        }
    }
    private static void replay() throws InterruptedException {
        Scanner in = new Scanner(System.in);
        String win = "";
        if (winner == 2)
            win = opponentname;
        else if (winner == 1)
            win = playername;
        System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
        System.out.println("--------------------------------------------------------------------------------");
        System.out.println("|                                   LAST MATCH                                 |");
        System.out.println("|------------------------------------------------------------------------------|");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|   PLAYER NAME: " + playername + "                                                     |");
        System.out.println("|   OPPONENT NAME: " + opponentname + "                                                 |");
        System.out.println("|                                                                              |");
        System.out.println("|   WINNER: " + win + "                                                              |");
        System.out.println("|   MOVES DONE: " + moves + "                                                             |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                              Press Enter to go to Menu                       |");
        System.out.println("--------------------------------------------------------------------------------");
        in.nextLine();
        mainmenu();
    }
