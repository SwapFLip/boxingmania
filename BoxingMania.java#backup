        if (rage_meter >= 0) {
            opponentHealth -= playerHook;
            BoxingMania.playSound("data/normalpunch.wav");
            BoxingMania.playSound("data/gethit.wav");
        } else {
            playerenergy -= 10;
            opponentHealth -= playerHook;
            if (crit >= 700) {
                opponentHealth -= 10;
                BoxingMania.playSound("data/normalpunch.wav");
                BoxingMania.playSound("data/gethit.wav");
            } else {
                BoxingMania.playSound("data/normalpunch.wav");
                BoxingMania.playSound("data/gethit.wav");
            }
        }
