            System.out.println("|                 " + playername + "                                " + opponentname + "            |");
             System.out.println("|                   " + playerHealth + " HP                                   " + opponentHealth + " HP            |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|            |==============|                         <==============>         |");
            System.out.println("|            |              |                         |              |         |");
            System.out.println("|            | " + name + " |                         |  " + ooface + "  |         |");
            System.out.println("|           (                )                       [                ]        |");
            System.out.println("|            |      " + nose + "      |                         |      ==      |         |");
            System.out.println("|            |    " + face + "    |                         |   " + oomouth + "   |         |");
            System.out.println("|            |______________|                         !______________!         |");
            System.out.println("|                                                                              |");
            System.out.println("|                               You did a Critical Hit!                        |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("--------------------------------------------------------------------------------");
            try {
                Thread.sleep(2000);
            } catch (InterruptedException e) {
                e.printStackTrace();
            }
        }
    }

