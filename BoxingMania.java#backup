            playerenergy += 2;
            opponentenergy += 2;
            if (playerenergy > 200) {
                playerenergy = 200;
            } else if (playerenergy < 0) playerenergy = 0;
            if (opponentenergy > 200) {
                opponentenergy = 200;
            } else if (opponentenergy < 0) opponentenergy = 0;
            playerHealth++;
            moves++;
            opponentHealth++;
        }
    }
    private static void start8() throws InterruptedException {
        //training
        playername = "Player";
        opponentname = "Dummy";
        BoxingMania.stopSound();
        playerHealth = 200;
        opponentHealth = 200;
        playerenergy = 200;
        opponentenergy = 200;
        if (battlemusic == 1) BoxingMania.playloopSound("data/fight.wav");
        else if (battlemusic == 2) BoxingMania.playloopSound("data/fight2.wav");
        else if (battlemusic == 3) BoxingMania.playloopSound("data/fight3.wav");
        else if (battlemusic == 4) {
            fights = (int)(Math.random() * 3) + 1;
            if (fights == 1) BoxingMania.playloopSound("data/fight.wav");
            else if (fights == 2) BoxingMania.playloopSound("data/fight2.wav");
            else if (fights == 3) BoxingMania.playloopSound("data/fight3.wav");
        }
        Scanner in = new Scanner(System.in);
        while (true) {
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
            System.out.println("|                 " + playername + "                                " + opponentname + "            |");
             System.out.println("|                   " + playerHealth + " HP                                   " + opponentHealth + " HP            |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|            |==============|     ENTER YOUR MOVE     <==============>         |");
            System.out.println("|            |              |                         |              |         |");
            System.out.println("|            | " + name + " |                         |  " + ooface + "  |         |");
            System.out.println("|           (                )                       [                ]        |");
            System.out.println("|            |      " + nose + "      |                         |      ==      |         |");
            System.out.println("|            |    " + face + "    |                         |   " + oomouth + "   |         |");
            System.out.println("|            |______________|                         !______________!         |");
            System.out.println("|                  " + playerenergy + " en                                  " + opponentenergy + " en            |");
            System.out.println("|______________________________________________________________________________|");
            System.out.println("|                                     MOVES                         10) Pause  |");
            System.out.println("|------------------------------------------------------------------------------|");
            System.out.println("|]        (1)        |        (2)       |        (3)       |        (4)       [|");
            System.out.println("|]    Jab - 20 en    |   Hook - 10 en   | Uppercut - 80 en |   Feint - 5 en   [|");
            System.out.println("|]-------------------|------------------|------------------|------------------[|");
            System.out.println("|]        (5)        |        (6)       |        (7)       |        (8)       [|");
            System.out.println("|]       Block       |   Sweep - 15 en  |   Kick - 20 en   |   Heal - 20 en   [|");
            System.out.println("--------------------------------------------------------------------------------");
            playerMove = in.nextInt();
            while (playerMove != 1 && playerMove != 2 && playerMove != 3 && playerMove != 4 && playerMove != 5 && playerMove != 6 && playerMove != 7 && playerMove != 8 && playerMove != 10) {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("|                 " + playername + "                                " + opponentname + "            |");
                 System.out.println("|                   " + playerHealth + " HP                                   " + opponentHealth + " HP            |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|            |==============|     ENTER YOUR MOVE     <==============>         |");
                System.out.println("|            |              |      Unknown Move!      |              |         |");
                System.out.println("|            | " + name + " |                         |  " + ooface + "  |         |");
                System.out.println("|           (                )                       [                ]        |");
                System.out.println("|            |      " + nose + "      |                         |      ==      |         |");
                System.out.println("|            |    " + face + "    |                         |   " + oomouth + "   |         |");
                System.out.println("|            |______________|                         !______________!         |");
                System.out.println("|                  " + playerenergy + " en                                  " + opponentenergy + " en            |");
                System.out.println("|______________________________________________________________________________|");
                System.out.println("|                                     MOVES                         10) Pause  |");
                System.out.println("|------------------------------------------------------------------------------|");
                System.out.println("|]        (1)        |        (2)       |        (3)       |        (4)       [|");
                System.out.println("|]    Jab - 20 en    |   Hook - 10 en   | Uppercut - 80 en |   Feint - 5 en   [|");
                System.out.println("|]-------------------|------------------|------------------|------------------[|");
                System.out.println("|]        (5)        |        (6)       |        (7)       |        (8)       [|");
                System.out.println("|]       Block       |   Sweep - 15 en  |   Kick - 20 en   |   Heal - 20 en   [|");
                System.out.println("--------------------------------------------------------------------------------");
                playerMove = in.nextInt();
            }
            while (playerMove == 1 && playerenergy < 20 || playerMove == 2 && playerenergy < 10 || playerMove == 3 && playerenergy < 80 || playerMove == 4 && playerenergy < 5 || playerMove == 6 && playerenergy < 15 || playerMove == 7 && playerenergy < 20 || playerMove == 8 && playerenergy < 20) {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("|                 " + playername + "                                " + opponentname + "            |");
                 System.out.println("|                   " + playerHealth + " HP                                   " + opponentHealth + " HP            |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|            |==============|     ENTER YOUR MOVE     <==============>         |");
                System.out.println("|            |              |        No Energy        |              |         |");
                System.out.println("|            | " + name + " |                         |  " + ooface + "  |         |");
                System.out.println("|           (                )                       [                ]        |");
                System.out.println("|            |      " + nose + "      |                         |      ==      |         |");
                System.out.println("|            |    " + face + "    |                         |   " + oomouth + "   |         |");
                System.out.println("|            |______________|                         !______________!         |");
                System.out.println("|                  " + playerenergy + " en                                  " + opponentenergy + " en            |");
                System.out.println("|______________________________________________________________________________|");
                System.out.println("|                                     MOVES                         10) Pause  |");
                System.out.println("|------------------------------------------------------------------------------|");
                System.out.println("|]        (1)        |        (2)       |        (3)       |        (4)       [|");
                System.out.println("|]    Jab - 20 en    |   Hook - 10 en   | Uppercut - 80 en |   Feint - 5 en   [|");
                System.out.println("|]-------------------|------------------|------------------|------------------[|");
                System.out.println("|]        (5)        |        (6)       |        (7)       |        (8)       [|");
                System.out.println("|]       Block       |   Sweep - 15 en  |   Kick - 20 en   |   Heal - 20 en   [|");
                System.out.println("--------------------------------------------------------------------------------");
                playerMove = in.nextInt();
            }
            if (playerMove == 10) {
                BoxingMania.stopSound();
                BoxingMania.pause();
            }
            while (opponentMove == 1 && opponentenergy < 20 || opponentMove == 2 && opponentenergy < 10 || opponentMove == 3 && opponentenergy < 80 || opponentMove == 4 && opponentenergy < 5 || opponentMove == 6 && opponentenergy < 15 || opponentMove == 7 && opponentenergy < 20 || opponentMove == 8 && opponentenergy < 20) {
                opponentMove = (int)(Math.random() * 8) + 1;
            }
            crit = (int)(Math.random() * 1000) + 1;
            if (playerMove == 1) {
                BoxingMania.move1();
            } else if (playerMove == 2) {
                BoxingMania.move2();
            } else if (playerMove == 3) {
                BoxingMania.move3();
            } else if (playerMove == 4) {
                BoxingMania.move4();
            } else if (playerMove == 6) {
                BoxingMania.move6();
            } else if (playerMove == 7) {
                BoxingMania.move7();
            } else if (playerMove == 8) {
                BoxingMania.move8();
            }
            if (playerMove == 5) {
                BoxingMania.move5();
            }
            if (opponentMove == 5) {
                BoxingMania.omove5();
            }
            playerenergy += 2;
            opponentenergy += 2;
            if (playerenergy > 200) {
                playerenergy = 200;
            } else if (playerenergy < 0) playerenergy = 0;
            if (opponentenergy > 200) {
                opponentenergy = 200;
            } else if (opponentenergy < 0) opponentenergy = 0;
            playerHealth = 200;
            opponentHealth = 200;
            if (playerHealth >= 1000) {
                playerHealth = 1000;
            }
            if (opponentHealth >= 1000) {
                opponentHealth = 1000;
            }
        }
    }
    private static void playSound(String fileName) throws InterruptedException {
        try {
            File soundFile = new File(fileName);
            AudioInputStream dataInputStream = AudioSystem.getAudioInputStream(soundFile);
            Clip clip = AudioSystem.getClip();
            clip.open(dataInputStream);
            clip.start();
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
    static Clip clip;
    static boolean playing = true;
    private static void playloopSound(String fileName) throws InterruptedException {
        File soundFile = new File(fileName);
        try {
            AudioInputStream dataIn = AudioSystem.getAudioInputStream(soundFile);
            clip = AudioSystem.getClip();
            clip.open(dataIn);
            clip.loop(Clip.LOOP_CONTINUOUSLY);
        } catch (UnsupportedAudioFileException | IOException | LineUnavailableException e) {
            e.printStackTrace();
        }
    }
    private static void stopSound() throws InterruptedException {
        clip.stop();
        clip.close();
    }
    private static void move1() throws InterruptedException {
        System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
        System.out.println("--------------------------------------------------------------------------------");
        System.out.println("|                 " + playername + "                                " + opponentname + "            |");
         System.out.println("|                   " + playerHealth + " HP                                   " + opponentHealth + " HP            |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|            |==============|                         <==============>         |");
        System.out.println("|            |              |                         |              |         |");
        System.out.println("|            | " + name + " |                         |  " + ooface + "  |         |");
        System.out.println("|           (                )                       [                ]        |");
        System.out.println("|            |      " + nose + "      |                         |      ==      |         |");
        System.out.println("|            |    " + face + "    |                         |   " + oomouth + "   |         |");
        System.out.println("|            |______________|                         !______________!         |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("--------------------------------------------------------------------------------");
        try {
            Thread.sleep(80);
