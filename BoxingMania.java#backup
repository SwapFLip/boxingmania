        opponentenergy -= 15;
        playerHealth -= opponentSweep;
        if (ocrit >= 700) {
            playerHealth -= 10;
            BoxingMania.playSound("data/normalpunch.wav");
            BoxingMania.playSound("data/gethit.wav");
        } else {
            BoxingMania.playSound("data/normalpunch.wav");
