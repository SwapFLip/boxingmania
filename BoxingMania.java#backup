        if (rage_meter >= 0) {
            opponentHealth -= playerJab;
            BoxingMania.playSound("data/normalpunch.wav");
            BoxingMania.playSound("data/gethit.wav");
        } else {
            playerenergy -= 20;
            opponentHealth -= playerJab;
            if (crit >= 700) {
                BoxingMania.playSound("data/normalpunch.wav");
                BoxingMania.playSound("data/gethit.wav");
                opponentHealth -= 10;
            } else {
                BoxingMania.playSound("data/normalpunch.wav");
                BoxingMania.playSound("data/gethit.wav");
