            if (opponentMove == 5) {
                BoxingMania.omove5();
            }
            playerenergy += 2;
            opponentenergy += 2;
            if (playerenergy > 200) {
                playerenergy = 200;
            } else if (playerenergy < 0) playerenergy = 0;
            if (opponentenergy > 200) {
                opponentenergy = 200;
            } else if (opponentenergy < 0) opponentenergy = 0;
            playerHealth++;
            opponentHealth++;
            if (playerHealth >= 1000 && playerNumber != 22) playerHealth = 1000;
            if (opponentHealth >= 1000 && opponentNumber != 22) opponentHealth = 1000;
            moves++;
        }
    }
    private static void begins() throws InterruptedException {
        if (battleintro == 0) {
            if (opponentNumber != 69)
                BoxingMania.playSound("data/battlestart.wav");
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                 BATTLE                                       |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|------------------------------------------------------------------------------|");
            System.out.println("|                                                                              |");
            System.out.println("--------------------------------------------------------------------------------");
            try {
                Thread.sleep(1000);
            } catch (InterruptedException e) {
                e.printStackTrace();
            }
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                 BATTLE BEGINS                                |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|------------------------------------------------------------------------------|");
            System.out.println("|                                                                              |");
            System.out.println("--------------------------------------------------------------------------------");
            try {
                Thread.sleep(1100);
            } catch (InterruptedException e) {
                e.printStackTrace();
            }
            if (opponentNumber == 69)
                BoxingMania.playSound("data/ui.wav");
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
            System.out.println("|           |______________|                         !______________!          |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                 BATTLE BEGINS                                |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
