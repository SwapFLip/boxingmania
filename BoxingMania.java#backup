            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                      ROUND 2                                 |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("--------------------------------------------------------------------------------");
            try {
                Thread.sleep(4000);
            } catch (InterruptedException e) {
                e.printStackTrace();
            }
            opponentHealth = op2[0];
            opponentJab = op2[1];
            opponentHook = op2[2];
            opponentUppercut = op2[3];
            opponentFeint = op2[4];
            opponentSweep = op2[5];
            opponentKick = op2[6];
            opponentHeal = op2[7];
            opponentname = op2n;
            tournafight();
        } else if (tourna == 4) {
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                      ROUND 3                                 |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("--------------------------------------------------------------------------------");
            try {
                Thread.sleep(4000);
            } catch (InterruptedException e) {
                e.printStackTrace();
            }
            opponentHealth = op3[0];
            opponentJab = op3[1];
            opponentHook = op3[2];
            opponentUppercut = op3[3];
            opponentFeint = op3[4];
            opponentSweep = op3[5];
            opponentKick = op3[6];
            opponentHeal = op3[7];
            opponentname = op3n;
            tournafight();
        } else if (tourna == 5) {
            coins += 100;
            diffc -= coins;
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                            CONGRATULATIONS! YOU WON!                         |");
            System.out.println("|                                      THE                                     |");
            System.out.println("|                                WORLD TOURNAMENT                              |");
            System.out.println("|                                                                              |");
            System.out.println("|                             AND FOR WINNING YOU GET                          |");
            System.out.println("|                                      " + coins + "                                     |");
            System.out.println("|                                     BCOINS!                                  |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                                              |");
            System.out.println("--------------------------------------------------------------------------------");
            tourna = 0;
            botaistrength = 2;
            diffc = 0;
            in.nextLine();
            in.nextLine();
            in.nextLine();
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/menu.wav");
            BoxingMania.mainmenu();
        }
    }
    private static void tournafight() throws InterruptedException {
        BoxingMania.stopSound();
        BoxingMania.playloopSound("data/tournament.wav");
        opponentenergy = 200;
        winner = 0;
        moves = 0;
        Scanner in = new Scanner(System.in);
        while (true) {
            playerMove = 99;
            while (playerMove != 1 && playerMove != 2 && playerMove != 3 && playerMove != 4 && playerMove != 5 && playerMove != 6 && playerMove != 7 && playerMove != 8 && playerMove != 10) {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("|                 " + playername + "                                " + opponentname + "            |");
                 System.out.println("|                   " + playerHealth + " HP                                   " + opponentHealth + " HP            |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|            |==============|     ENTER YOUR MOVE     <==============>         |");
                System.out.println("|            |              |                         |              |         |");
                System.out.println("|            | " + name + " |                         |  " + ooface + "  |         |");
                System.out.println("|           (                )                       [                ]        |");
                System.out.println("|            |      " + nose + "      |                         |      ==      |         |");
                System.out.println("|            |    " + face + "    |                         |   " + oomouth + "   |         |");
                System.out.println("|            |______________|                         !______________!         |");
                System.out.println("|                  " + playerenergy + " en                                  " + opponentenergy + " en            |");
                System.out.println("|______________________________________________________________________________|");
                System.out.println("|                                     MOVES                         10) Pause  |");
                System.out.println("|------------------------------------------------------------------------------|");
                System.out.println("|]        (1)        |        (2)       |        (3)       |        (4)       [|");
                System.out.println("|]    Jab - 20 en    |   Hook - 10 en   | Uppercut - 80 en |   Feint - 5 en   [|");
                System.out.println("|]-------------------|------------------|------------------|------------------[|");
                System.out.println("|]        (5)        |        (6)       |        (7)       |        (8)       [|");
                System.out.println("|]       Block       |   Sweep - 15 en  |   Kick - 20 en   |   Heal - 20 en   [|");
                System.out.println("--------------------------------------------------------------------------------");
                playerMove = in.nextInt();
            }
            while (playerMove == 1 && playerenergy < 20 || playerMove == 2 && playerenergy < 10 || playerMove == 3 && playerenergy < 80 || playerMove == 4 && playerenergy < 5 || playerMove == 6 && playerenergy < 15 || playerMove == 7 && playerenergy < 20 || playerMove == 8 && playerenergy < 20) {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("|                 " + playername + "                                " + opponentname + "            |");
                 System.out.println("|                   " + playerHealth + " HP                                   " + opponentHealth + " HP            |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|            |==============|     ENTER YOUR MOVE     <==============>         |");
                System.out.println("|            |              |        No Energy        |              |         |");
                System.out.println("|            | " + name + " |                         |  " + ooface + "  |         |");
                System.out.println("|           (                )                       [                ]        |");
                System.out.println("|            |      " + nose + "      |                         |      ==      |         |");
                System.out.println("|            |    " + face + "    |                         |   " + oomouth + "   |         |");
                System.out.println("|            |______________|                         !______________!         |");
                System.out.println("|                  " + playerenergy + " en                                  " + opponentenergy + " en            |");
                System.out.println("|______________________________________________________________________________|");
                System.out.println("|                                     MOVES                         10) Pause  |");
                System.out.println("|------------------------------------------------------------------------------|");
                System.out.println("|]        (1)        |        (2)       |        (3)       |        (4)       [|");
                System.out.println("|]    Jab - 20 en    |   Hook - 10 en   | Uppercut - 80 en |   Feint - 5 en   [|");
                System.out.println("|]-------------------|------------------|------------------|------------------[|");
                System.out.println("|]        (5)        |        (6)       |        (7)       |        (8)       [|");
                System.out.println("|]       Block       |   Sweep - 15 en  |   Kick - 20 en   |   Heal - 20 en   [|");
                System.out.println("--------------------------------------------------------------------------------");
                playerMove = in.nextInt();
            }
            if (playerMove == 10) {
                BoxingMania.stopSound();
                BoxingMania.pause();
            }
            //opponent ai
            if (botaistrength == 0)
                BoxingMania.ailvnoob();
            if (botaistrength == 1)
                BoxingMania.ailv1();
            if (botaistrength == 2)
                BoxingMania.ailv2();
            if (botaistrength == 3)
                BoxingMania.ailv3();
            if (botaistrength == 4)
                BoxingMania.ailv4();
            if (opponentMove != 5) {
                crit = (int)(Math.random() * 1000) + 1;
                if (playerMove == 1) {
                    BoxingMania.move1();
                } else if (playerMove == 2) {
                    BoxingMania.move2();
                } else if (playerMove == 3) {
                    BoxingMania.move3();
                } else if (playerMove == 4) {
                    BoxingMania.move4();
                } else if (playerMove == 6) {
                    BoxingMania.move6();
                } else if (playerMove == 7) {
                    BoxingMania.move7();
                } else if (playerMove == 8) {
                    BoxingMania.move8();
                }
            } else if (playerMove == 8) {
                BoxingMania.move8();
            }
            if (playerHealth <= 0) {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                       KO                                     |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                             YOU LOST THE TOURNAMENT                          |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("--------------------------------------------------------------------------------");
                try {
                    Thread.sleep(4000);
                } catch (InterruptedException e) {
                    e.printStackTrace();
                }
                winner = 2;
                in.nextLine();
                in.nextLine();
                coins -= 100;
                BoxingMania.stopSound();
                BoxingMania.playloopSound("data/menu.wav");
                BoxingMania.mainmenu();
            } else if (opponentHealth <= 0) {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                       KO                                     |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                              YOU WON THE TOURNAMENT!                         |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("--------------------------------------------------------------------------------");
                try {
                    Thread.sleep(4000);
                } catch (InterruptedException e) {
                    e.printStackTrace();
                }
                in.nextLine();
                winner = 1;
                if (playerNumber != 22 || opponentNumber != 22) {
                    if (botaistrength == 0)
                        coins += 80;
                    else if (botaistrength == 1)
                        coins += 120;
                    else if (botaistrength == 2)
                        coins += 150;
                    else if (botaistrength == 3)
                        coins += 180;
                    else if (botaistrength == 4)
                        coins += 200;
                    if (opponentNumber == 5 || opponentNumber == 6 || opponentNumber == 7)
                        coins += 20;
                    else if (opponentNumber == 8 || opponentNumber == 9)
                        coins += 35;
                    else if (opponentNumber == 10 || opponentNumber == 69)
                        coins += 50;
                }
                coins += 50;
                stopSound();
                BoxingMania.playloopSound("data/menu.wav");
                BoxingMania.tournament();
            }
            if (playerMove != 5) {
                ocrit = (int)(Math.random() * 1000) + 1;
                if (opponentMove == 1) {
                    BoxingMania.omove1();
                } else if (opponentMove == 2) {
                    BoxingMania.omove2();
                } else if (opponentMove == 3) {
                    BoxingMania.omove3();
                } else if (opponentMove == 4) {
                    BoxingMania.omove4();
                } else if (opponentMove == 6) {
                    BoxingMania.omove6();
                } else if (opponentMove == 7) {
                    BoxingMania.omove7();
                } else if (opponentMove == 8) {
                    BoxingMania.omove8();
                }
            } else if (opponentMove == 8) {
                BoxingMania.omove8();
            }
            if (playerHealth <= 0) {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                       KO                                     |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                             YOU LOST THE TOURNAMENT                          |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("--------------------------------------------------------------------------------");
                try {
                    Thread.sleep(4000);
                } catch (InterruptedException e) {
                    e.printStackTrace();
                }
                winner = 2;
                in.nextLine();
                coins -= 100;
                BoxingMania.stopSound();
                BoxingMania.playloopSound("data/menu.wav");
                BoxingMania.mainmenu();
            } else if (opponentHealth <= 0) {
                System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
                System.out.println("--------------------------------------------------------------------------------");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                       KO                                     |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                              YOU WON THE TOURNAMENT!                         |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("|                                                                              |");
                System.out.println("--------------------------------------------------------------------------------");
                try {
                    Thread.sleep(4000);
                } catch (InterruptedException e) {
                    e.printStackTrace();
                }
                winner = 1;
                in.nextLine();
                if (playerNumber != 22 || opponentNumber != 22) {
                    if (botaistrength == 0)
                        coins += 80;
                    else if (botaistrength == 1)
                        coins += 120;
                    else if (botaistrength == 2)
                        coins += 150;
                    else if (botaistrength == 3)
                        coins += 180;
                    else if (botaistrength == 4)
                        coins += 200;
                    if (opponentNumber == 5 || opponentNumber == 6 || opponentNumber == 7)
                        coins += 20;
                    else if (opponentNumber == 8 || opponentNumber == 9)
                        coins += 35;
                    else if (opponentNumber == 10 || opponentNumber == 69)
                        coins += 50;
                }
                coins += 50;
                stopSound();
                BoxingMania.playloopSound("data/menu.wav");
                BoxingMania.tournament();
            }
            if (playerMove == 5) BoxingMania.move5();
            if (opponentMove == 5) BoxingMania.omove5();
            playerenergy += 2;
            opponentenergy += 2;
            if (playerenergy > 200) playerenergy = 200;
            else if (playerenergy < 0) playerenergy = 0;
            if (opponentenergy > 200) opponentenergy = 200;
            else if (opponentenergy < 0) opponentenergy = 0;
            playerHealth++;
            opponentHealth++;
            moves++;
            if (playerHealth >= 1000 && playerNumber != 22) playerHealth = 1000;
            if (opponentHealth >= 1000 && opponentNumber != 22) opponentHealth = 1000;
        }
    }
    private static void openWebpage(String url) throws InterruptedException {
        try {
            Desktop.getDesktop().browse(new URI(url));
        } catch (IOException | URISyntaxException e) {
            e.printStackTrace();
        }
    }
}