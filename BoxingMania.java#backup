    private static boolean isInternetAvailable() {
        try {
            InetAddress address = InetAddress.getByName("www.google.com");
            return !address.equals("");
        } catch (UnknownHostException e) {
            return false;
        }
    }
    private static void music() throws InterruptedException {
        Scanner in = new Scanner(System.in);
        if (secretBOSSFIGHT == 1)
            secretBOSSFIGHT = 2;
        String m = "p";
        while ((!m.equals("1")) && (!m.equals("2")) && (!m.equals("3")) && (!m.equals("4")) && (!m.equals("5")) && (!m.equals("6")) && (!m.equals("7")) && (!m.equals("8")) && (!m.equals("9")) && (!m.equals("10")) && (!m.equals("11")) && (!m.equals("12")) && (!m.equals("13")) && (!m.equals("14"))) {
            System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
            System.out.println("--------------------------------------------------------------------------------");
            System.out.println("|                                                                    10) Menu  |");
            System.out.println("|   MUSIC PLAYER                                                               |");
            System.out.println("|                                                                              |");
            System.out.println("|  1) Click Sound                                                              |");
            System.out.println("|  2) RAVE by Dxrk                                                             |");
            System.out.println("|  3) Fight Song 1                                                             |");
            System.out.println("|  4) Fight Song 2                                                             |");
            System.out.println("|  5) Fight Song 3                                                             |");
            System.out.println("|  6) Get Hit Sound                                                            |");
            System.out.println("|  7) Game Intro                                                               |");
            System.out.println("|  8) Main Menu Sound                                                          |");
            System.out.println("|  9) Punch Sound                                                              |");
            System.out.println("|  12) Battle Starting                                                         |");
            System.out.println("|  13) Tournament Theme                                                        |");
            System.out.println("|  14) Ultra Instinct                                                          |");
            System.out.println("|                                                                              |");
            System.out.println("|                                                          11) Stop all sounds |");
            System.out.println("--------------------------------------------------------------------------------");
            m = in.nextLine();
        }
        if (m.equals("1")) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/click.wav");
            BoxingMania.music();
        } else if (m.equals("2")) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/DxrkRAVE.wav");
            if (secretBOSSFIGHT == 2)
                secretBOSSFIGHT = 3;
            BoxingMania.music();
        } else if (m.equals("3")) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/fight.wav");
            BoxingMania.music();
        } else if (m.equals("4")) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/fight2.wav");
            BoxingMania.music();
        } else if (m.equals("5")) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/fight3.wav");
            BoxingMania.music();
        } else if (m.equals("6")) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/gethit.wav");
            BoxingMania.music();
        } else if (m.equals("7")) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/intro.wav");
            BoxingMania.music();
        } else if (m.equals("8")) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/menu.wav");
            BoxingMania.music();
        } else if (m.equals("9")) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/normalpunch.wav");
            BoxingMania.music();
        } else if (m.equals("10")) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/menu.wav");
            BoxingMania.mainmenu();
        } else if (m.equals("11")) {
            BoxingMania.stopSound();
            BoxingMania.music();
        } else if (m.equals("12")) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/battlestart.wav");
            BoxingMania.music();
        } else if (m.equals("13")) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/tournament.wav");
            BoxingMania.music();
        } else if (m.equals("14")) {
            BoxingMania.stopSound();
            BoxingMania.playloopSound("data/ui.wav");
            BoxingMania.music();
        }
    }
    private static void start10() throws InterruptedException {
        Scanner in = new Scanner(System.in);
        if (savevalue1 < 1) {
            onewin = "Not Done";
        } else {
            onewin = "Completed!";
        }
        if (savevalue2 < 1) {
            oneloss = "Not Done";
        } else {
            oneloss = "Completed!";
        }
        if (savevalue1 < 10) {
            tenwin = "Not Done";
        } else {
            tenwin = "Completed!";
        }
        if (savevalue2 < 10) {
            tenloss = "Not Done";
        } else {
            tenloss = "Completed!";
        }
        if (savevalue1 < 50) {
            fiftywin = "Not Done";
        } else {
            fiftywin = "Completed!";
        }
        if (savevalue2 < 50) {
            fiftyloss = "Not Done";
        } else {
            fiftyloss = "Completed!";
        }
        if (savevalue1 < 100) {
            hundreadwin = "Not Done";
        } else {
            hundreadwin = "Completed!";
        }
        if (savevalue2 < 100) {
            hundreadloss = "Not Done";
        } else {
            hundreadloss = "Completed!";
        }
        System.out.println("\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n");
        System.out.println("--------------------------------------------------------------------------------");
        System.out.println("|                                                                              |");
        System.out.println("|                                 ACHIEVEMENTS                                 |");
        System.out.println("|                                                                              |");
        System.out.println("|    You got a Win? (Get 1 win) = " + onewin + "                                     |");
        System.out.println("|    You Must like the game :) (Get 10 wins) = " + tenwin + "                        |");
        System.out.println("|    Whoa Stop! Isnt it too much? (Get 50 wins) = " + fiftywin + "                   |");
        System.out.println("|    Stop Winning? Can you? (Get 100 wins) = " + hundreadwin + "                     |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|    Got Har Har Har from Coach! (Lost once) = " + oneloss + "                       |");
        System.out.println("|    Lol You Suck (Lost 10 times) = " + tenloss + "                                  |");
        System.out.println("|    You Must be disastrous (Lose 50 times) = " + fiftyloss + "                      |");
        System.out.println("|    Your Awful (Get 100 losses) = " + hundreadloss + "                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                            Enter to Return to Menu                           |");
        System.out.println("--------------------------------------------------------------------------------");
        in.nextLine();
        BoxingMania.stopSound();
        BoxingMania.playloopSound("data/menu.wav");
        BoxingMania.mainmenu();
    }
    static int playerNumber = 0;
    static int oface = 0,
        omouth = 0;
    static String ooface = "";
    static String oomouth = "";
    static int[] op1 = new int[8];
    static String op1n = "";
    static int[] op2 = new int[8];
    static String op2n = "";
    static int[] op3 = new int[8];
    static String op3n = "";
    private static void start1() throws InterruptedException {
        oface = (int)(Math.random() * 10) + 1;
        omouth = (int)(Math.random() * 10) + 1;
        if (oface == 1) ooface = oeyes1;
        else if (oface == 2) ooface = oeyes2;
        else if (oface == 3) ooface = oeyes3;
        else if (oface == 4) ooface = oeyes4;
        else if (oface == 5) ooface = oeyes5;
        else if (oface == 6) ooface = oeyes6;
        else if (oface == 7) ooface = oeyes7;
        else if (oface == 8) ooface = oeyes8;
        else if (oface == 9) ooface = oeyes9;
        else if (oface == 10) ooface = oeyes10;
