        System.out.println("|            |    " + face + "    |    |---|    |   " + oomouth + "   |                     |");
        System.out.println("|            |______________|             !______________!                     |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("|                                                                              |");
        System.out.println("--------------------------------------------------------------------------------");
        try {
